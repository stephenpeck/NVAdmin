{include file=$SESSION.ENVIRONMENT.EN_HEADER} <div class="row">  <div class="col-sm-1" ></div>  <div class="col-sm-10" >  		<table class="table table-striped table-bordered table-hover">			<thead>			<form action={$SESSION.PostAction} method=post class="form-inline" role="form">			<input type=hidden name=action value="ListEnvironments">		      <tr>		        <td colspan="10" >		        		<div class="form-group col-sm-2"> Name</div>		        		<div class="form-group col-sm-2"> <input type=text class="form-control" name=arCtl[EN_NAME] value="{$arCtl.EN_NAME}"> </div>		        		<div class="form-group col-sm-8"> <button type="submit" class="btn btn-default ">Show Environments</button></td> </div>		        </td>		      </tr>		 </form>							<tr >					<td class="col-md-2">Name</td>					<td class="col-md-2">URL</td>					<td class="col-md-2">Company</td>					<td class="col-md-2">PathFile</td>					<td class="col-md-2">CSS</td>					<td class="col-md-1" ></td>						<td class="col-md-1" ></td>					</tr>			</thead>				{section name=row loop=$arENVIRONMENTS}					<form action="{$SESSION.PostAction}" method="post">					<input type="hidden" name="ENVIRONMENTS[EN_KEY]" value="{$arENVIRONMENTS[row].EN_KEY}" />					<input type="hidden" name="action" value="UpdEnvironments" />						<tr >							<td><input type=text class="form-control" name=ENVIRONMENTS[EN_NAME] value="{$arENVIRONMENTS[row].EN_NAME}"></td>							<td><input type=text class="form-control" name=ENVIRONMENTS[EN_URL] value="{$arENVIRONMENTS[row].EN_URL}"></td>							<td>{html_options class="form-control" name=ENVIRONMENTS[EN_COKEY] options=$arCompanyList selected=$arENVIRONMENTS[row].EN_COKEY}</td>							<td><input type=text class="form-control" name=ENVIRONMENTS[EN_PATHFILE] value="{$arENVIRONMENTS[row].EN_PATHFILE}"></td>							<td><input type=text class="form-control" name=ENVIRONMENTS[EN_CSS] value="{$arENVIRONMENTS[row].EN_CSS}"></td>							<td rowspan=2> <a href="#{$arENVIRONMENTS[row].EN_KEY}" data-toggle="collapse"> Show Paras</A> </td>							<td ><button type="submit" class="btn btn-primary">Update</button> </td>						</tr >						<tr >							<td> Logo</td>							<td><input type=text class="form-control" name=ENVIRONMENTS[EN_LOGO] value="{$arENVIRONMENTS[row].EN_LOGO}"></td>							<td> Header \ Footer</td>							<td><input type=text class="form-control" name=ENVIRONMENTS[EN_HEADER] value="{$arENVIRONMENTS[row].EN_HEADER}"></td>							<td><input type=text class="form-control" name=ENVIRONMENTS[EN_FOOTER] value="{$arENVIRONMENTS[row].EN_FOOTER}"></td>					</form>					<form action="{$SESSION.PostAction}" method="post">					<input type="hidden" name="ENVIRONMENTS[EN_KEY]" value="{$arENVIRONMENTS[row].EN_KEY}" />					<input type="hidden" name="action" value="DelEnvironments" />							<td ><button type="submit" class="btn btn-primary">Del</button> </td>						</tr>					</form>																				<tr class="collapse" id={$arENVIRONMENTS[row].EN_KEY}>						<td></td>						<td colspan=5>							<table class="table table-striped table-bordered table-hover">								<tr>									<th> Function </th>									<th> Description </th>									<th> Value </th>									<td  ></td>									</tr>								{assign var=arENVIRONMENTPARAMETERS value=$arENVIRONMENTS[row].ENVIRONMENTPARAMETERS}								{section name=row2 loop=$arENVIRONMENTPARAMETERS}									<form action="{$SESSION.PostAction}" method="post">									<input type="hidden" name="ENVIRONMENTS[EN_KEY]" value="{$arENVIRONMENTS[row].EN_KEY}" />									<input type="hidden" name="ENVIRONMENT_PARAMETERS[EP_KEY]" value="{$arENVIRONMENTPARAMETERS[row2].EP_KEY}" />									<input type="hidden" name="action" value="DelEnvironmentParameters" />										<tr >											<td> {$arENVIRONMENTPARAMETERS[row2].PA_NAME} </th>											<td> {$arENVIRONMENTPARAMETERS[row2].PA_DESCRIPTION} </th>											<td> {$arENVIRONMENTPARAMETERS[row2].EP_VALUE} </th>											<td><button type="submit" class="btn btn-primary">Del</button> </td>										</tr>									</form>								{/section}													<form action="{$SESSION.PostAction}" method="post">								<input type="hidden" name="ENVIRONMENTS[EN_KEY]" value="{$arENVIRONMENTS[row].EN_KEY}" />								<input type="hidden" name="ENVIRONMENT_PARAMETERS[EP_KEY]" value="" />								<input type="hidden" name="ENVIRONMENT_PARAMETERS[EP_ENKEY]" value="{$arENVIRONMENTS[row].EN_KEY}" />								<input type="hidden" name="action" value="UpdEnvironmentParameters" />									<tr >										<td colspan=2> {html_options class="form-control" name=ENVIRONMENT_PARAMETERS[EP_PAKEY] options=$arParameterList} </th>										<td> <input type="text" class="form-control" name="ENVIRONMENT_PARAMETERS[EP_VALUE]" />										<td><button type="submit" class="btn btn-primary">Add</button> </td>									</tr>								</form>							</table>							</td>					<tr>																			{/section}						<tr >				<form action="{$SESSION.PostAction}" method=post>				<input type="hidden" name="ENVIRONMENTS[EN_KEY]" value="" />				<input type="hidden" name="action" value="UpdEnvironments" />					<tr >						<td><input type=text class="form-control" name=ENVIRONMENTS[EN_NAME] ></td>						<td><input type=text class="form-control" name=ENVIRONMENTS[EN_URL] ></td>						<td>{html_options class="form-control" name=ENVIRONMENTS[EN_COKEY] options=$arCompanyList }</td>						<td><input type=text class="form-control" name=ENVIRONMENTS[EN_PATHFILE] }"></td>						<td><input type=text class="form-control" name=ENVIRONMENTS[EN_CSS] ></td>						<td colspan="2"> <button type="submit" class="btn btn-primary">Add</button> </td>				</form>			</tr>			</table>  </div>  <div class="col-sm-1" ></div></div>	{include file=$SESSION.ENVIRONMENT.EN_FOOTER}